<div class="main-content">
  <div class="cards-listings">
   <% @listings.each do |listing| %>
    <div class="col-2">
     <%= link_to listing_path(listing) do %>
        <div align="center">
          <div class="container-fx">
          <div class="image-fx" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url('<%= cl_image_path listing.photos[0].key %>')">
            <div class="overlay overlayFade">
              <div class="text-card">
              <h4><%= listing.name %></h4>
              <p><%= listing.address %></p>
              </div>
            </div>
          </div>
        </div>
      <% end %>
      </div>
    <% end %>
  </div>
    <div id="map"
      data-markers="<%= @markers.to_json %>"
      data-mapbox-api-key="<%= ENV['MAPBOX_API_KEY'] %>">
    </div>
</div>
